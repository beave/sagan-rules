alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[ZINGBOX] Anonymous netbios-ssn login attempt detected"; content: "Anonymous netbios-ssn login attempt"; parse_src_ip:1; classtype: suspicious-traffic; program: zws; reference: url,wiki.quadrantsec.com/bin/view/Main/5004305; sid:5004305; rev: 2;) 

alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[ZINGBOX] HTTP connection to risky Internet site detected"; content: "HTTP connection to risky Internet site detecte"; parse_src_ip:1; classtype: suspicious-traffic; program: zws; reference: url,wiki.quadrantsec.com/bin/view/Main/5004306; sid:5004306; rev: 2;) 

alert any $EXTERNAL_NET any -> $HOME_NET any (msg: "[ZINGBOX] https connection to risky Internet site detected"; content: "https connection to risky Internet site detected"; parse_src_ip:1; classtype: suspicious-traffic; program: zws; reference: url,wiki.quadrantsec.com/bin/view/Main/5004307; sid:5004307; rev: 2;) 

