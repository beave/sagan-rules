# Windows Websense rules.   
# Eventlog to syslog service.  This is what we primarily use.
# http://code.google.com/p/eventlog-to-syslog/

alert tcp $HOME_NET any -> $EXTERNAL_NET any (msg: "[WINDOWS-WEBSENSE] RDP / Logon type 10 from a Websense listed IP"; program: Security*; pcre: "/ 528: | 4624: /"; content: "Logon Type|3a| 10 "; websense: by_src, $WEBSENSE_NETWORK; program: Security*; parse_src_ip: 1; normalize: windows; classtype: successful-user; reference: url,wiki.quadrantsec.com/bin/view/Main/5002232; sid: 5002232; rev:1;)
alert syslog $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-WEBSENSE] Login failure from a Websense listed IP [0/5]"; program: Security*; content: " 529|3a| "; classtype: unsuccessful-user; websense: by_src, $WEBSENSE_NETWORK; threshold: type limit, track by_src, count 5, seconds 300; fwsam: src, 1 day; normalize: windows; parse_src_ip: 1; parse_port; reference: url,wiki.quadrantsec.com/bin/view/Main/5002233; sid: 5002233; rev:1;)
alert syslog $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-WEBSENSE] Login failure from a Websense listed IP [Time restriction] [0/5]"; content: " 530|3a| "; classtype: unsuccessful-user; program: Security*; parse_src_ip: 1; websense: by_src, $WEBSENSE_NETWORK; threshold: type limit, track by_src, count 5, seconds 300; fwsam: src, 1 day; normalize: windows; reference: url,wiki.quadrantsec.com/bin/view/Main/5002234; sid: 5002234; rev:4;)
alert syslog $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-WEBSENSE] Login failure from Websense listed IP - Account currently disabled [0/5]"; content: " 531|3a| "; content:!"User Name|3a| Domain|3a|"; classtype: unsuccessful-user; program: Security*; websense: by_src, $WEBSENSE_NETWORK; normalize: windows; parse_src_ip: 1; threshold: type limit, track by_src, count 5, seconds 300; reference: url,wiki.quadrantsec.com/bin/view/Main/5002235; sid: 5002235; rev:1;)
alert syslog $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-WEBSENSE] Login failure from Websense listed IP - Specified account expired"; content: " 532|3a| "; classtype: unsuccessful-user; program: Security*; websense: by_src, $WEBSENSE_NETWORK; normalize: windows; parse_src_ip: 1; reference: url,wiki.quadrantsec.com/bin/view/Main/5002236; sid: 5002236; rev:1;)
alert syslog $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-WEBSENSE] Login failure from Websense listed IP - User not allowed to login at this computer"; content: " 533|3a| "; classtype: unsuccessful-user; program: Security*; websense: by_src, $WEBSENSE_NETWORK; normalize: windows; parse_src_ip: 1; reference: url,wiki.quadrantsec.com/bin/view/Main/5002237; sid: 5002237; rev:1;)
alert syslog $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-WEBSENSE] Login failure from Websense listed IP - Account locked [0/1]"; content: " 539|3a| "; content:!"User Name|3a| Domain|3a| Logon Type|3a|"; classtype: unsuccessful-user; websense: by_src, $WEBSENSE_NETWORK; threshold: type limit, track by_src, count 5, seconds 300; parse_src_ip: 1; parse_port; normalize: windows; fwsam: src, 1 day; program: Security*; reference: url,wiki.quadrantsec.com/bin/view/Main/5002238; sid: 5002238; rev:1;)
alert syslog $EXTERNAL_NET any -> $HOME_NET any (msg: "[WINDOWS-WEBSENSE] Windows DC Logon Failure from Websense listed IP"; pcre: "/ 675: | 676: | 681: /"; classtype: unsuccessful-user; program: Security*; websense: by_src, $WEBSENSE_NETWORK; normalize: windows; parse_src_ip: 1; reference: url,wiki.quadrantsec.com/bin/view/Main/5002239; sid: 5002239; rev:1;)


